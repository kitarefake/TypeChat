{
  "name": "typechat",
  "version": "0.0.10",
  "description": "An exploration of using schemas to make prompt engineering simpler and more reliable.",
  "main": "./build/typechat.js",
  "files": [
    "build/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "compile": "tsc",
    "lint": "gts lint",
    "clean": "gts clean",
    "fix": "gts fix",
    "postbuild": "copyfiles src/**/*Schema.ts build",
    "postcompile": "copyfiles src/**/*Schema.ts build",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microsoft/typechat.git"
  },
  "keywords": [
    "schema",
    "LLM",
    "prompt",
    "TypeScript",
    "validation"
  ],
  "author": "Microsoft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/microsoft/typechat/issues"
  },
  "homepage": "https://github.com/microsoft/typechat#readme",
  "dependencies": {
    "axios": "^1.4.0",
    "azure-openai": "^0.9.4",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^18.15.12",
    "copyfiles": "^2.4.1",
    "gts": "^3.1.1",
    "jest": "^29.5.0",
    "spotify-types": "^1.0.7",
    "ts-jest": "^29.1.0",
    "typescript": "~4.7.0"
  }
}
